{
  "name": "@mistericy/renegade-diff",
  "description": "Advanced AI-powered code review system that autonomously analyzes your pull requests",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/mistericy/renegade-diff",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistericy/renegade-diff.git"
  },
  "bugs": {
    "url": "https://github.com/mistericy/renegade-diff/issues"
  },
  "main": "index.js",
  "scripts": {
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write .",
    "local-action": "npx local-action . src/main.ts .env",
    "package": "npx ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "bundle": "npm run format:write && npm run package"
  },
  "keywords": [
    "actions",
    "generative ai",
    "code review"
  ],
  "author": "Alexandros Koutroulis",
  "license": "GPL-2.0-only",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "openai": "^4.73.0"
  },
  "devDependencies": {
    "@github/local-action": "^2.2.1",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.1",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  }
}
